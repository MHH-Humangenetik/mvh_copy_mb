INPUT_DIR=./data
GPAS_ENDPOINT=https://gpas.example.com/gpas/PseudonymService?wsdl
GPAS_USER=myuser
GPAS_PASSWORD=mypassword
GPAS_GRZ=grz
GPAS_KDK=ccdn
GPAS_VERIFY_SSL=false
LOG_LEVEL=INFO

# Web Server Configuration
WEB_HOST=0.0.0.0
WEB_PORT=8000
WEB_RELOAD=false
WEB_LOG_LEVEL=info
DB_PATH=./data/meldebestaetigungen.duckdb

# Multi-User Synchronization Configuration
SYNC_WEBSOCKET_HOST=localhost
SYNC_WEBSOCKET_PORT=8001
SYNC_WEBSOCKET_PATH=/ws
SYNC_CONNECTION_TIMEOUT=300
SYNC_HEARTBEAT_INTERVAL=30
SYNC_MAX_CONNECTIONS_PER_USER=5
SYNC_LOCK_TIMEOUT=30
SYNC_LOCK_CLEANUP_INTERVAL=60
SYNC_MAX_BATCH_SIZE=100
SYNC_BATCH_TIMEOUT=100
SYNC_EVENT_BUFFER_SIZE=1000
SYNC_MAX_RECONNECTION_ATTEMPTS=5
SYNC_INITIAL_RECONNECTION_DELAY=1.0
SYNC_MAX_RECONNECTION_DELAY=30.0
SYNC_RECONNECTION_BACKOFF_MULTIPLIER=2.0
SYNC_LOG_LEVEL=INFO
SYNC_LOG_SYNC_EVENTS=true
SYNC_LOG_CONNECTION_EVENTS=true
SYNC_LOG_CONFLICT_EVENTS=true
SYNC_ENABLE_EVENT_BATCHING=true
SYNC_ENABLE_CONNECTION_POOLING=true
SYNC_ENABLE_METRICS_COLLECTION=true